<?xml version="1.0" encoding="utf-8"?>
<activity name="ActionsView" clr-ns="odm.ui.activities">
	<use clr-ns="onvif.services"/>
	<model>
    <prop name="actions" clr-type="Action1[]"/>
    <prop name="selection" clr-type="Action1" />
    <prop name="supportedActions" clr-type="SupportedActions"/>
	</model>
  <result>
    <option name="Create">
      <param name="model" clr-type="Model"/>
    </option>
    <option name="Delete">
      <param name="model" clr-type="Model"/>
    </option>
    <option name="Modify">
      <param name="model" clr-type="Model"/>
    </option>
    <option name="Close"/>
  </result>
</activity>
