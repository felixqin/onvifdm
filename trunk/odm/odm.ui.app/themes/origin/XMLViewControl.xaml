<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:cmd="clr-namespace:Microsoft.Practices.Prism.Commands;assembly=Microsoft.Practices.Prism"
                    xmlns:odm_ui_controls="clr-namespace:odm.ui.controls;assembly=odm.ui.views">
    <Style TargetType="{x:Type odm_ui_controls:ControlXMLView}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type odm_ui_controls:ControlXMLView}">
                    <WebBrowser odm_ui_controls:WebBrowserHelper.Body ="{TemplateBinding StringInfo}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>